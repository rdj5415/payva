# API Configuration
REACT_APP_API_URL=https://api.payva.com
REACT_APP_API_VERSION=v1

# Authentication
REACT_APP_AUTH_DOMAIN=your-auth-domain.auth0.com
REACT_APP_AUTH_CLIENT_ID=your-client-id
REACT_APP_AUTH_AUDIENCE=your-api-audience

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_NOTIFICATIONS=true

# External Services
REACT_APP_PLAID_ENV=sandbox
REACT_APP_PLAID_PUBLIC_KEY=your-plaid-public-key

# Monitoring
REACT_APP_SENTRY_DSN=your-sentry-dsn
REACT_APP_LOGROCKET_ID=your-logrocket-id

# Build Configuration
REACT_APP_ENV=production
REACT_APP_VERSION=$npm_package_version

REACT_APP_GA_TRACKING_ID=your-ga-tracking-id 